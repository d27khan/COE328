reg CLRN;
reg E;
// wires                                               
wire [0:2] Q;
wire [3:0] STUDENT_ID;

wire sampler;                             

// assign statements (if any)                          
Johnsschem i1 (
// port map - connection between master ports and signals/registers   
	.CLKN(CLKN),
	.CLRN(CLRN),
	.E(E),
	.Q(Q),
	.STUDENT_ID(STUDENT_ID)
);

// CLKN
initial
begin
	CLKN = 1'b0;
	CLKN = #10000 1'b1;
	CLKN = #5000 1'b0;
	# 5000;
	repeat(8)
	begin
		CLKN = 1'b1;
		CLKN = #5000 1'b0;
		# 5000;
	end
	CLKN = 1'b1;
	CLKN = #5000 1'b0;
end 

// CLRN
initial
begin
	CLRN = 1'b0;
	CLRN = #10000 1'b1;
	CLRN = #50000 1'b0;
	CLRN = #10000 1'b1;
end 

// E
initial
begin
	E = 1'b0;
	E = #10000 1'b1;
end 

Johnsschem_vlg_sample_tst tb_sample (
	.CLKN(CLKN),
	.CLRN(CLRN),
	.E(E),
	.sampler_tx(sampler)
);

Johnsschem_vlg_check_tst tb_out(
	.Q(Q),
	.STUDENT_ID(STUDENT_ID),
	.sampler_rx(sampler)
