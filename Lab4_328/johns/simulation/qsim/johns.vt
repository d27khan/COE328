`timescale 1 ps/ 1 ps
module Johnsschem_vlg_sample_tst(
	CLKN,
	CLRN,
	E,
	sampler_tx
);
input  CLKN;
input  CLRN;
input  E;
output sampler_tx;

reg sample;
time current_time;
always @(CLKN or CLRN or E)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module Johnsschem_vlg_check_tst (
	Q,
	STUDENT_ID,
	sampler_rx
);
input [0:2] Q;
input [3:0] STUDENT_ID;
input sampler_rx;

reg [0:2] Q_expected;
reg [3:0] STUDENT_ID_expected;

reg [0:2] Q_prev;
reg [3:0] STUDENT_ID_prev;

reg [0:2] Q_expected_prev;
reg [3:0] STUDENT_ID_expected_prev;

reg [0:2] last_Q_exp;
reg [3:0] last_STUDENT_ID_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:2] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 2'b1;
end

// update real /o prevs

always @(trigger)
begin
	Q_prev = Q;
	STUDENT_ID_prev = STUDENT_ID;
end

// update expected /o prevs

always @(trigger)
begin
	Q_expected_prev = Q_expected;
	STUDENT_ID_expected_prev = STUDENT_ID_expected;
end


// expected Q[ 2 ]
initial
begin
	Q_expected[2] = 1'b0;
	Q_expected[2] = #30000 1'b1;
	Q_expected[2] = #30000 1'b0;
	Q_expected[2] = #30000 1'b1;
end 
// expected Q[ 1 ]
