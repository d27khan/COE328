		$display ("     Expected value = %b", STUDENT_ID_expected_prev);
		$display ("     Real value = %b", STUDENT_ID_prev);
		nummismatches = nummismatches + 1;
		on_first_change[2] = 1'b0;
		last_STUDENT_ID_exp[2] = STUDENT_ID_expected_prev[2];
